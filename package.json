{
  "name": "yangone",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm",
    "update:deps": "npx taze -r -w",
    "version": "pnpm exec changeset version && pnpm install --no-frozen-lockfile",
    "catalog": "pnpx codemod pnpm/catalog"
  },
  "dependencies": {
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/tsconfig": "catalog:",
    "@yangone/commitlint-config": "workspace:*",
    "@yangone/eslint-config": "workspace:*",
    "@yangone/node-utils": "workspace:*",
    "@yangone/prettier-config": "workspace:*",
    "@yangone/stylelint-config": "workspace:*",
    "@yangone/tsconfig": "workspace:*",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "catalog:"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
